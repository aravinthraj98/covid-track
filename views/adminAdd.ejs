  <div class="container">
   
    <form class="my-2 w-75 m-auto" id="form" method="post" onsubmit="event.preventDefault()">
      <div class="jumbotron mt-3">
        <p id="demo"></p>
        <div class="form-group row">
          <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
          <div class="col-sm-10">
            <input type="email" class="form-control" id="Email" required />
          </div>
        </div>
            <div class="form-group row">
              <label for="phonenumber" class="col-sm-2 col-form-label">phone</label>
              <div class="col-sm-10">
                <input type="number" class="form-control" id="phonenumber" minlength="10" maxlength="10" />
              </div>
            </div>

        <fieldset class="form-group">
          <div class="row">
            <legend class="col-form-label col-sm-2 pt-0">Address</legend>
            <div class="col-sm-10">
              <div class="form-group">
                <div class="form-row">
                  <div class="col-5">
                    <input type="text" class="form-control" id="dno" placeholder="Door Number" required />
                  </div>
                  <div class="col-7">
                    <input type="text" class="form-control" id="street" placeholder="Street" required />
                  </div>
                </div>
              </div>
              <div class="form-group">
                <input type="text" class="form-control" id="area" placeholder="Area" required />
              </div>
              <div class="form-group">
                <div class="form-row">
                  <div class="col-5">
                    <input type="text" class="form-control" id="city" placeholder="City" required />
                  </div>
                  <div class="col-5">
                    <input type="text" class="form-control" id="state" placeholder="State" required />
                  </div>
                  <div class="col">
                    <input type="number" class="form-control" id="zip" placeholder="Zip" required />
                  </div>
                </div>
              </div>
            </div>
          </div>
        </fieldset>

        <div class="form-group row">
          <label for="familyCount" class="col-sm-2 col-form-label">Family Count</label>
          <div class="col-sm-10">
            <input type="number" class="form-control" id="familyCount" min="1" max="10" value=0 />
          </div>
        </div>

        <div class="form-group row">
          <label for="names" class="col-sm-2 col-form-label">Names</label>
          <div class="col-sm-10">
            <input type="type" class="form-control" id="names" placeholder="Enter coma separarted values" required />
          </div>
        </div>
        <ul id="list"></ul>
      </div>
      <div class="form-group row ml-1">
        <button onclick="checkfunction()" class="btn btn-primary" id="submit">
          Submit
        </button>
        <button class="btn ml-2 btn-secondary"  onclick="getlocation();event.preventDefault();">getlocation</button>
       
      </div>


    </form>
   

  </div>
  <script src="/assets/js/adminvalidate.js"></script>
   
 
  <script src=https://code.jquery.com/jquery-3.5.1.min.js></script>
